<?xml version="1.0"?>
<jboss>
	<enterprise-beans>
		<session>
			<ejb-name>ContestPipelineServiceBean</ejb-name>
			<jndi-name>remote/ContestPipelineServiceBean</jndi-name>

			<ejb-ref>
				<ejb-ref-name>ejb/CatalogService</ejb-ref-name>
				<jndi-name>CatalogService/remote</jndi-name>
			</ejb-ref>
			<ejb-ref>
				<ejb-ref-name>ejb/ProjectServicesBean</ejb-ref-name>
