			</ejb-ref>

			<port-component>
				<port-component-name>ContestPipelineServiceBean</port-component-name>
				<port-component-uri>/ContestPipelineServiceBean</port-component-uri>
				<transport-guarantee>CONFIDENTIAL</transport-guarantee>
			</port-component>
		</session>
	</enterprise-beans>
	
