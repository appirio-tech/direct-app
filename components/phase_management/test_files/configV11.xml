						<Value>informix</Value>
					</Property>
					<Property name="password">
						<Value>123456</Value>
					</Property>
				</Property>
			</Property>
		</Property>
	</Config>

	<Config name="com.topcoder.db.connectionfactory.DBConnectionFactoryImpl.invalid">
		<Property name="connections">
