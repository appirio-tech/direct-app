<?xml version="1.0" ?>
<CMConfig>
    <Config name="com.topcoder.service.facade.contest.ejb.ContestServiceFacadeBean">
        <Property name="ContestEligibilityMapping">
            <Property name="@ContestEligibilityMapping.name@">
              <Property name="EligibilityName">
                <Value>@ContestEligibilityMapping.EligibilityName@</Value>
              </Property>
              <Property name="EligibilityGroupId">
                <Value>@ContestEligibilityMapping.EligibilityGroupId@</Value>
              </Property>
              <Property name="EligibilityAdminRole">
                <Value>@ContestEligibilityMapping.EligibilityAdminRole@</Value>
              </Property>
            </Property>
        </Property>
        <Property name="PayflowProPaymentProcessor">
		        <Property name="payFlowHostAddress">
		            <Value>@payFlowHostAddress@</Value>
		        </Property>
		        <Property name="payFlowUser">
		            <Value>@payFlowUser@</Value>
		        </Property>
		        <Property name="payFlowPartner">
		            <Value>@payFlowPartner@</Value>
		        </Property>
		        <Property name="payFlowVendor">
		            <Value>@payFlowVendor@</Value>
		        </Property>
		        <Property name="payFlowPassword">
		            <Value>@payFlowPassword@</Value>
		        </Property>
        </Property>
        <Property name="createForum">
        	<Value>@createForum@</Value>
        </Property>
        <Property name="forumBeanProviderUrl">
        	<Value>@forumBeanProviderUrl@</Value>
        </Property>
        <Property name="userBeanProviderUrl">
        	<Value>@userBeanProviderUrl@</Value>
        </Property>
        <Property name="projectBeanProviderUrl">
        	<Value>@projectBeanProviderUrl@</Value>
        </Property>
        <Property name="mockSubmissionFilePath">
        	<Value>@mockSubmissionFilePath@</Value>
        </Property>
        <Property name="contestCreationSNS">
            <Property name="contestCreationArn">
                <Value>@contestCreationArn@</Value>
            </Property>
            <Property name="contestCreationSubject">
                <Value>@contestCreationSubject@</Value>
            </Property>
            <Property name="contestUpdateSubject">
                <Value>@contestUpdateSubject@</Value>
            </Property>
            <Property name="contestCreationMessageTemplatePath">
                <Value>@contestCreationMessageTemplatePath@</Value>
            </Property>
            <Property name="contestUpdateMessageTemplatePath">
                <Value>@contestUpdateMessageTemplatePath@</Value>
            </Property>
        </Property>        
    </Config>
</CMConfig>
